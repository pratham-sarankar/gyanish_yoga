              </li>
            </ul>
            <% if(status=="success"){ %>
            <div class="nav-link d-flex">
              <a
                href="/users/profile"
                class="ti-user"
                style="
                  border-radius: 50%;
                  padding: 10px;
                  color: #fff;
                  background-color: rgb(234, 59, 97);
                "
              ></a>
            </div>
            <% } else {%>
            <a
              href="/loginRegister"
              id="loginButton"
              class="btn btn-default rounded-0 animation nav-link"
              data-animation="fadeInUp"
              data-animation-delay="0.7s"
            >
              Login
            </a>
            <% } %>
